{
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.safiastore.shop"
        }
      ],
      "destination": "https://safiastore.shop/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "query",
          "key": "subdomain"
        }
      ],
      "destination": "/:path*",
      "permanent": true
    }
  ]
}
